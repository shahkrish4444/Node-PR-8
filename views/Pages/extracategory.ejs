<%- include('../partials/Header.ejs') -%>
    <div class="page-wrapper mt-80 p-4">

        <h2 class="text-light mb-4">Add Extra Category</h2>
        <form method="post" action="/extracategory">
            <input type="text" name="name" placeholder="Enter Category Name" class="form-control mb-3">
            <select name="subCategoryId" id="" class="form-control mb-3">
                <% subdata.forEach((data,i)=>{%>
                    <option value=<%=data.id %>><%= data.name %>
                    </option>
                    <% }) %>
            </select>
            <button type="submit" class="btn btn-light">ADD</button>
        </form>
    </div>
    <%- include('../partials/Footer.ejs') -%>